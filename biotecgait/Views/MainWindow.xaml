            <GradientStop Color="Black" Offset="1"/>
            <GradientStop Color="#FF38387C" Offset="0.012"/>
        </LinearGradientBrush>
    </Window.Background>

    
    <Grid HorizontalAlignment="Center" Height="521" VerticalAlignment="Top" Width="900">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="163*"/>
            <ColumnDefinition Width="737*"/>
        </Grid.ColumnDefinitions>
        <TextBlock HorizontalAlignment="Left" Height="38" Margin="10,19,0,0" TextWrapping="Wrap" Text="Highlights" VerticalAlignment="Top" Width="100" Foreground="White" FontSize="18" FontFamily="Segoe UI Black" Opacity="0.505">
            <TextBlock.Effect>
                <hc:BrightnessEffect/>
            </TextBlock.Effect>
        </TextBlock>
        <StackPanel Margin="10,58,2,20">
            <Button Style="{DynamicResource ButtonStyle6}" Content="Button" Width="133" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White"/>
            <Button Style="{DynamicResource ButtonStyle4}" Content="Button" Width="133" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White"/>
            <Button Style="{DynamicResource ButtonStyle7}" Content="Button" Width="133" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White"/>
            <Button Style="{DynamicResource ButtonStyle8}" Content="Button" Width="133" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White"/>
        </StackPanel>

        <ContentControl Grid.Column="1" Content="{Binding CurrentView}"/>

    </Grid>
</Window>
